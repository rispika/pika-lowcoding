<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pika.generate.mapper.GenerateColMapper">

    <resultMap type="com.pika.generate.entity.GenerateCol" id="PikaGenerateColMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="type" column="type" jdbcType="INTEGER"/>
        <result property="length" column="length" jdbcType="INTEGER"/>
        <result property="isPrimary" column="is_primary" jdbcType="INTEGER"/>
        <result property="isAutoIncrement" column="is_auto_increment" jdbcType="INTEGER"/>
        <result property="defaultValue" column="default_value" jdbcType="VARCHAR"/>
        <result property="comment" column="comment" jdbcType="VARCHAR"/>
        <result property="sqlId" column="sql_id" jdbcType="INTEGER"/>
    </resultMap>
    <select id="queryAllTablesInfo" resultType="java.lang.String">
        show tables
    </select>


</mapper>

